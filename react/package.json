{
  "private": true,
  "scripts": {
    "start": "cross-env APP_ENV=local umi dev",
    "build": "cross-env APP_ENV=pro umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^6.5.0",
    "ahooks": "^3.4.1",
    "axios": "^0.27.2",
    "cross-env": "^7.0.3",
    "lodash": "^4.17.21",
    "moment": "^2.29.3",
    "react": "17.x",
    "react-dom": "17.x",
    "rxjs": "^7.5.5",
    "umi": "^3.5.25"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.182",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.5.25",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
